
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<header>
    <title>Đơn hàng</title>
    <script src="~/Scripts/jquery.tmpl.js"></script>
    <script src="~/Scripts/Product/Index.js"></script>
</header>
<body>
    <div class="col-lg-6">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h4>Danh sách khách hàng</h4>
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover">
                        <thead id="TableTitle">
                            <tr>
                                <th>ID</th>
                                <th>Name product</th>
                                <th>Unit Price</th>
                                <th>Package</th>
                                <th>Is Discontinued</th>
                                <th>Company Name</th>
                                <th>Supplier City/Country</th>
                                <!--Title bảng-->
                            </tr>
                        </thead>
                        <script type="text/html" id="Content">
                            <tr>
                                <td>${ProductSMR.Id}</td>
                                <td>${ProductSMR.ProductName}</td>
                                <td>${ProductSMR.UnitPrice}</td>
                                <td>${ProductSMR.Package}</td>
                                <td>${ProductSMR.IsDiscontinued}</td>
                                <td>${SupplierSMR.CompanyName}</td>
                                <td>${SupplierSMR.City}/${SupplierSMR.Country}</td>
                            </tr>
                            <!--Nôi dung bảng-->
                        </script>
                    </table>
                </div>
                <!-- /.table-responsive -->
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
</body>



